        {
            "name": "CLI: Command line",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "--actions", "export",
                "--region", "europe",
                "--datasets", "poi", 
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}